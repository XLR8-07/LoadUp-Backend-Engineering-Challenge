{
    "name": "job-application-service",
    "version": "1.0.0",
    "description": "Production-ready HTTP service for job applications with auto-scoring",
    "main": "dist/server.js",
    "scripts": {
        "dev": "tsx watch src/server.ts",
        "build": "tsc",
        "start": "node dist/server.js",
        "test": "vitest",
        "test:watch": "vitest --watch",
        "lint": "eslint src --ext .ts",
        "db:setup": "node scripts/setup-db.js",
        "db:migrate": "node scripts/migrate.js",
        "db:clean": "node scripts/clean-db.js",
        "setup": "npm install && npm run db:setup && npm run db:migrate"
    },
    "keywords": [
        "typescript",
        "express",
        "jobs",
        "scoring"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "pg": "^8.11.3",
        "swagger-ui-express": "^5.0.0",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@types/express": "^4.17.21",
        "@types/node": "^20.10.5",
        "@types/pg": "^8.10.9",
        "@types/swagger-ui-express": "^4.1.6",
        "@types/uuid": "^9.0.7",
        "tsx": "^4.7.0",
        "typescript": "^5.3.3",
        "vitest": "^1.1.0"
    }
}